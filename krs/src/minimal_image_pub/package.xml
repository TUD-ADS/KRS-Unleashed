<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>minimal_image_pub</name>
  <version>0.1.0</version>
  <description> Simple Image Pub for Testing
  </description>
  <maintainer email="paul.gottschaldt@tu-dresden.de">Paul Gottschaldt</maintainer>
  <license>Apache License 2.0</license>
  <author email="paul.gottschaldt@tu-dresden.de">Paul Gottschaldt</author>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_cmake_auto</buildtool_depend>

  <depend>cv_bridge</depend>
  <depend>image_geometry</depend>
  <depend>image_transport</depend>
  <depend>rcutils</depend>
  <depend>sensor_msgs</depend>
  <depend>rclcpp</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
